{
  "name": "zoe-solar-accounting-ocr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "check": "npm run typecheck && npm run build",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "lint-staged": "npx lint-staged",

    "ğŸš€ AUTO-SWARM COMMANDS": "========================================",
    "swarm": "node .claude/parallel-swarm.js",
    "auto-swarm": "node .claude/auto-swarm-executor.js",
    "master-loop": "node .claude/master-loop-executor.js",

    "ğŸ¯ VISION QUALITY GATE": "========================================",
    "vision-gate": "node .claude/vision-workflow.js",
    "vision-check": "node .claude/vision-workflow.js --check",
    "vision-fix": "node .claude/vision-workflow.js --fix",

    "ğŸ›¡ï¸ BIG PICKLE HANDOVER": "========================================",
    "setup-big-pickle": "node .claude/explizit-handover.js",
    "handover": "node .claude/explizit-handover.js",
    "handover:force": "node .claude/explizit-handover.js --force",
    "handover:log": "cat .claude/handover-log.md",

    "ğŸ“‹ CONDUCTOR WORKFLOW": "========================================",
    "conductor:setup": "bash .claude/setup-auto-swarm.sh",
    "conductor:newTrack": "npx gemini-cli /conductor:newTrack",
    "conductor:implement": "npx gemini-cli /conductor:implement",
    "conductor:status": "npx gemini-cli /conductor:status",
    "conductor:revert": "npx gemini-cli /conductor:revert",

    "ğŸš€ DEPLOYMENT": "========================================",
    "deploy:prod": "npm run build && vercel --prod --yes",
    "deploy:full": "npm run vision-gate && vercel --prod --yes",
    "deploy:with-check": "npm run check && npm run vision-gate && vercel --prod --yes",

    "ğŸ“Š MONITORING": "========================================",
    "monitor": "python3 ~/.claude/skyvern_monitor.py --url https://zoe-solar-accounting-ocr.vercel.app",
    "monitor:logs": "cat ~/.claude/MONITORING_REPORT.md",
    "status": "ls -la .claude/checkpoints/ && cat ~/.claude/TASK_QUEUE.yaml 2>/dev/null || echo 'No active tasks'",

    "ğŸ“š KNOWLEDGE": "========================================",
    "knowledge:update": "cat .claude/PROJECT_KNOWLEDGE.md",
    "knowledge:errors": "cat ~/.claude/ERROR_SOLUTIONS.md",
    "knowledge:research": "cat .claude/RESEARCH_LOG.md",

    "ğŸ”§ SETUP & HELP": "========================================",
    "setup": "bash .claude/setup-auto-swarm.sh",
    "help": "echo 'ğŸš€ Usage: npm run master-loop \"[Aufgabe]\"' && echo 'ğŸ¯ Swarm: npm run swarm \"[Aufgabe]\"' && echo 'ğŸ¨ Vision: npm run vision-gate' && echo 'ğŸ›¡ï¸ Handover: npm run handover'",

    "âš¡ QUICK COMMANDS": "========================================",
    "quick-fix": "npm run master-loop \"Fix Supabase Connection\"",
    "quick-build": "npm run check && npm run deploy:prod",
    "quick-deploy": "npm run build && vercel --prod --yes",
    "quick-status": "npm run status && npm run knowledge:errors"
  },
  "dependencies": {
    "@google/genai": "^1.33.0",
    "@supabase/supabase-js": "^2.89.0",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.23",
    "jspdf": "^4.0.0",
    "jspdf-autotable": "^5.0.7",
    "pdfjs-dist": "^5.4.530",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^22.14.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-perf": "^3.2.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "husky": "^9.0.11",
    "jsdom": "^27.4.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.6",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vitest": "^4.0.16"
  }
}
